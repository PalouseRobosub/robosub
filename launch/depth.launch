<?xml version="1.0"?>
<!-- This file launches the sensor node with the specified serial port. -->

<launch>
    <arg name="name" default="depth" />
    <arg name="port" default="/dev/$(arg name)" />
    <rosparam file="$(find robosub)/param/depth.yaml" />
    <rosparam file="$(find robosub)/param/cobalt.yaml" />
    <node name="$(arg name)" pkg="rosserial_python" type="serial_node.py" args="$(arg port)" />
</launch>
